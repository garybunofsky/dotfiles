# alias

# git
alias status="git status"
alias push="git push"
alias pull="git pull"
alias fetch="git checkout"
alias checkout="git checkout"
alias reset="git reset"
alias diff="git diff"
alias oneline="git log --oneline"
alias graph="git log --all --decorate --oneline --graph"
alias graph-mini="git log --all --decorate --graph --oneline --pretty=format:"%C(yellow)%h%Creset%C(blue)%d%Creset""

# bash
alias projects="cd ~/documents/projects/"
alias insights="cd ~/documents/projects/insights/"
alias intel="cd ~/documents/projects/intel/"
alias sosa="cd ~/documents/projects/sosa/"
alias ops="cd ~/documents/projects/utees-operations"
alias htdocs="cd /Applications/MAMP/htdocs"
alias championone="cd /Applications/MAMP/htdocs/champion-one/wp-content/themes/championone"
alias clear_swap="find . -type f -name \"*.sw[klmnop]\" -delete"
alias o="open ."
alias t="bundle exec rspec"
alias ..="cd .."

# vim
alias vi="vim"
